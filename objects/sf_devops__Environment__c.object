    <validationRules>
        <fullName>sf_devops__Expired_Changed</fullName>
        <active>true</active>
        <description>An Environment cannot have its Expired__c field set to false.</description>
        <errorConditionFormula>AND(NOT(sf_devops__Expired__c), ISCHANGED(sf_devops__Expired__c))</errorConditionFormula>
        <errorMessage>An Environment cannot have its Expired__c field set to false.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Expired_Insert</fullName>
        <active>true</active>
        <description>An Environment cannot be inserted with Expired__c set to true.</description>
        <errorConditionFormula>AND(sf_devops__Expired__c, ISNEW())</errorConditionFormula>
        <errorMessage>An Environment cannot be inserted with Expired__c set to true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Expired_Replaces</fullName>
        <active>true</active>
        <description>A replacement Environment must be expired.</description>
        <errorConditionFormula>AND(OR(ISNEW(),ISCHANGED(sf_devops__Replaces__c)), NOT(sf_devops__Replaces__r.sf_devops__Expired__c))</errorConditionFormula>
        <errorMessage>A replacement Environment must be expired.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Refresh_Source_And_Date</fullName>
        <active>true</active>
        <description>Verify both Refresh_Source and Refresh_Date are set, or both are empty</description>
        <errorConditionFormula>AND(
    OR(ISNEW(), ISCHANGED(sf_devops__Refresh_Source__c), ISCHANGED(sf_devops__Refresh_Date__c)),
     ISBLANK( sf_devops__Refresh_Date__c )  &lt;&gt;  ISBLANK( sf_devops__Refresh_Source__c ) 
  )</errorConditionFormula>
        <errorMessage>Refresh_Source and Refresh_Date most both be set, or both be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sf_devops__Unique_Name</fullName>
        <active>false</active>
        <description>This rule is no longer enforced</description>
        <errorConditionFormula>FALSE</errorConditionFormula>
        <errorMessage>Deprecated</errorMessage>
    </validationRules>
